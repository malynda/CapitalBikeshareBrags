{
  "manifest_version": 2,

  "name": "CapitalBikeshareBrags",
  "description": "Liberate your bikeshare data.",
  "version": "1.0.0",

  "icons": {
    "48": "icon48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://secure.capitalbikeshare.com/profile/trips"],
      "js": [
        "jquery.js",
        "highcharts.js",
        "jquery.livequery.js",
        "background.js"
      ],
      "css": ["background.css"]
    }
  ],

  "web_accessible_resources": [
    "jquery.js",
    "jquery.livequery.js",
    "highcharts.js",
    "background.js",
    "background.css",
    "ajax-loader.gif",
    "twitter_logo_white.png",
    "star_icon_white.png",
    "api_key.json"
  ],

  "permissions": [
    "http://divvybrags-leaderboard.herokuapp.com/",
    "https://maps.googleapis.com/maps/api/distancematrix/json",
    "storage"
  ]

}
